        rdfs:comment "Any agentive Object , either physical (e.g. a whale, a robot, an oak), or social (e.g. a corporation, an institution, a community)."^^xsd:string,
        rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl>,
        rdfs:label "Agent"@en,
        rdfs:label "Agente"@it
    
    SubClassOf: 
        Object
    
    
